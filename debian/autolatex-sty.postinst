#!/bin/sh -e
# 
# postinst maintainer script for the Debian autolatex-sty package.

case "$1" in
  configure|abort-*)

    mktexlsr
    echo "Creating missing formats."
    update-updmap --quiet
    ;;
  *)
    echo "autolatex-sty: postinst called with unknown argument: '$1''" >&2
    ;;
esac


